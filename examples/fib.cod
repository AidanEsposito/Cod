ocean number fibonacci(n){
    reel fibHelper(a, b, count) {
        if count == 0 {
        reel a
    }
    reel fibHelper(b, a + b, count - 1)
    }(0, 1, n)
}

ocean number printFibonacci(count){
    if count <= 0{
        cast: "Please enter a postive number"
        reel
    }

    cast: "Fibonacci Sequence:"
    stream i in 0 ..< count {
        cast: fibonacci(i)
    }
}

printFibonacci(10)
